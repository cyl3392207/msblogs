{
  "if": {
    "allOf": [
      {
        "source": "action",
        "like": "Microsoft.Cache/Redis/*"
      },
      {
        "not": {
          "allOf": [
            {
              "field": "Microsoft.Cache/Redis/sku.name",
              "in": [ "Basic", "Standard" ]
            },
            {
               "field": "Microsoft.Cache/Redis/enableNonSslPort",
              "in": [ "Basic", "Standard" ]
            }
          ]
        }
      }
    ]
  },
  "then": {
    "effect": "deny"
  }
}
